#include<stdio.h>
int main()
{
	int a[1000],i,n,sum1=0,sum2=0;
	char ge;
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
	  scanf("%d",&a[i]);
    }
       scanf(" %c",&ge);
    for(i=0;i<n;i++)
    {
        if(i%2!=0)
        {
        sum2=sum2+a[i];
        }
      
        else
        {
        sum1=sum1+a[i];
        }
     }
       if(ge=='b')
       printf("%d ",sum1);
         if(ge=='g')
      printf("%d",sum2); 
    return 0;
}
